{
  "name": "basitune",
  "version": "1.0.26",
  "description": "A lightweight desktop wrapper for YouTube Music",
  "type": "module",
  "scripts": {
    "dev": "tauri dev --no-watch",
    "dev:watch": "tauri dev",
    "build": "node ./scripts/set-build-version.js && tauri build",
    "build:fast": "cd src-tauri && cargo build --profile release-fast",
    "build:optimized": "npm run build",
    "tauri": "tauri",
    "icon": "tauri icon icon.svg"
  },
  "devDependencies": {
    "@tauri-apps/cli": "^2.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/basitune.git"
  },
  "keywords": [
    "youtube-music",
    "tauri",
    "desktop-app",
    "music",
    "electron-alternative"
  ],
  "author": "Basitune Contributors",
  "license": "MIT"
}
